{
    "name": "nietonfir/google-recaptcha-bundle",
    "description": "Symfony2 bundle for integrating nietonfir/google-recaptcha (simplifying the server-side validation of Google's 'No CAPTCHA reCAPTCHA')",
    "keywords": [
        "google",
        "recaptcha",
        "captcha",
        "No CAPTCHA reCAPTCHA",
        "security"
    ],
    "homepage": "https://github.com/nietonfir/GoogleReCaptchaBundle",
    "license": "MIT",
    "authors": [
        {
            "name"    : "nietonfir",
            "email"   : "nietonfir@gmail.com",
            "homepage": "http://nietonfir.eu",
            "role"    : "developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/config": "~3.0",
        "symfony/form": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/validator": "~3.0",
        "nietonfir/google-recaptcha": "v0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Nietonfir\\Google\\ReCaptchaBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Nietonfir\\Google\\ReCaptchaBundle\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
